#include<bits/stdc++.h>
#include<iostream>
using namespace std;


int main()
{
    
    
int t;
cin>>t;
while(t--)
{
    int a,b,count=0;
    cin>>a>>b;
    if(a==b)
    {
        count=0;
    }
    else if(a<b && (b-a)%2==1)
    {
        count=1;
    }
    else if(a>b && (a-b)%2==0)
    {
        count=1;
    }
    else
    {
        count=2;
    }
    cout<<count<<endl;
}
       
    return 0;
}
